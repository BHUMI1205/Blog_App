<script type="text/javascript">
  $(function() {

var startDatePicker = moment().utc();
var endDatePicker = moment().utc();

function updateDateRange(start, end) {
  $('#reportrange span').html(start.format('MMMM D, YYYY') + ' - ' + end.format('MMMM D, YYYY'));
}

function updateHiddenInputs(start, end) {
  var isoStartDate = start.toISOString();
  var isoEndDate = end.toISOString();

  var startDateObject = new Date(isoStartDate);
  var endDateObject = new Date(isoEndDate);
  endDateObject.setUTCHours(23, 59, 59, 0);
  startDateObject.setUTCHours(0, 0, 0, 0);

  $('input[name="startDate"]').val(startDateObject.toISOString());
  $('input[name="endDate"]').val(endDateObject.toISOString());
}

$('#reportrange').daterangepicker({
  startDate: startDatePicker,
  endDate: endDatePicker,
  ranges: {
    'Today': [moment().utc(), moment().utc()],
    'Yesterday': [moment().utc().subtract(1, 'days'), moment().utc().subtract(1, 'days')],
    'Last 7 Days': [moment().utc().subtract(6, 'days'), moment().utc()],
    'Last 30 Days': [moment().utc().subtract(29, 'days'), moment().utc()],
    'This Month': [moment().utc().startOf('month'), moment().utc().endOf('month')],
    'Last Month': [moment().utc().subtract(1, 'month').startOf('month'), moment().utc().subtract(1, 'month').endOf('month')]
  }
}, function(start, end) {
  updateDateRange(start, end);
  updateHiddenInputs(start, end);
});

updateDateRange(startDatePicker, endDatePicker);
updateHiddenInputs(startDatePicker, endDatePicker);

});

</script>




